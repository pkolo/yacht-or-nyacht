<div class="song-list-section">
  <div class="row list-headers">
    <div class="song-list-box col-1 yachtski-column">Yachtski</div>
    <div class="song-list-box col-1 hideable">Track</div>
    <div class="song-list-box col-5 title">Song</div>
    <div class="hideable">
      <div class="song-list-box col-1">JD</div>
      <div class="song-list-box col-1">Hunter</div>
      <div class="song-list-box col-1">Steve</div>
      <div class="song-list-box col-1">Dave</div>
      <div class="song-list-box col-1">Ep #</div>
    </div>
  </div>

  <div class="song-list">
    <% songs.each do |song| %>
     <%= erb :'albums/_list_item', locals: {song: song} %>
    <% end %>
  </div>
</div>
